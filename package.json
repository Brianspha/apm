{
  "name": "@brianspha/apm",
  "author": "Siphamandla Mjoli",
  "version": "0.1.0",
  "description": "A CLI tool for uploading packages to Arweave",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "bin": "bin/apm",
  "keywords": [
    "cli",
    "arweave",
    "decentralised",
    "storage",
    "package"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "preferGlobal": true,
  "main": "cli/apm.js",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "arql-ops": "^1.0.0",
    "arweave": "^1.7.1",
    "axios": "^0.19.2",
    "cache-manager": "^3.3.0",
    "cache-manager-fs": "^1.0.8",
    "child_process": "^1.0.2",
    "colors": "^1.4.0",
    "commander": "^5.1.0",
    "core-js": "^3.6.4",
    "create-file": "^1.0.1",
    "esm": "^3.2.25",
    "file-system-cache": "^1.0.5",
    "inquirer": "^7.1.0",
    "inquirer-fuzzy-path": "^2.3.0",
    "node-cache": "^5.1.0",
    "node-targz": "^0.2.0",
    "ora": "^4.0.4",
    "pack": "^2.2.0",
    "platform-folders": "^0.5.1",
    "read-package-json": "^2.1.1",
    "readjson": "^2.0.1",
    "vue": "^2.6.11",
    "vuetify": "^2.6.10",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "bin/",
    "cli/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianspha/apm.git"
  }
}
